{
  "name": "VoxelX",
  "version": "0.0.11",
  "description": "VoxelX",
  "main": "dist/main/main.js",
  "author": "FoxStudio",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run electron\"",
    "dev:vite": "vite",
    "electron": "tsc -p tsconfig.main.json && electron .",
    "build": "vite build && tsc -p tsconfig.main.json && electron-builder",
    "preview": "npm run build && electron .",
    "build:win": "npm run build && electron-builder --win",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^7.0.0",
    "@types/cors": "^2.8.19",
    "@types/discord-rpc": "^4.0.10",
    "@types/electron": "^1.6.12",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.4",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/semver": "^7.7.1",
    "@types/three": "^0.181.0",
    "@types/unzipper": "^0.10.11",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "electron": "^33.0.2",
    "electron-builder": "^26.0.12",
    "framer-motion": "^11.18.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@types/jimp": "^0.2.1",
    "@xmcl/core": "^2.15.1",
    "@xmcl/installer": "^6.1.2",
    "@xmcl/launch": "^1.1.1",
    "@xmcl/task": "^4.1.1",
    "@xmcl/user": "^4.3.0",
    "adm-zip": "^0.5.16",
    "antd": "^6.1.1",
    "archiver": "^7.0.1",
    "archiver-utils": "^5.0.2",
    "compare-versions": "^6.1.1",
    "cors": "^2.8.5",
    "discord-rpc": "^4.0.1",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "express": "^5.2.1",
    "jimp": "^1.6.0",
    "lucide-react": "^0.555.0",
    "minecraft-java-core": "^4.2.3",
    "minecraft-launcher-core": "^3.18.2",
    "minecraft-launcher-lib": "^0.4.0",
    "minecraft-player": "^1.0.1",
    "msmc": "^5.0.5",
    "nbt": "^0.8.1",
    "node-datachannel": "^0.31.0",
    "node-fetch": "^2.6.7",
    "public-ip": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-minecraft": "^1.1.0",
    "react-skinview3d": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "semver": "^7.7.3",
    "simple-peer": "^9.11.1",
    "skin3d": "^0.0.12",
    "skinview3d": "^3.4.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.2",
    "three": "^0.181.2",
    "unzipper": "^0.12.3",
    "uuid": "^8.3.2",
    "ws": "^8.18.3"
  },
  "build": {
    "appId": "com.voxelx.engine",
    "productName": "VoxelX",
    "copyright": "Copyright Â© 2025 FoxStudio",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "extraResources": [
      {
        "from": "src/renderer/assets/logo.png",
        "to": "assets/logo.png"
      },
      {
        "from": "src/renderer/assets/logo.ico",
        "to": "assets/logo.ico"
      },
      {
        "from": ".env",
        "to": ".env"
      }
    ],
    "files": [
      "dist/main/**/*",
      "dist/renderer/**/*",
      "dist/preload.js",
      "node_modules/**/*",
      "package.json",
      ".env"
    ],
    "win": {
      "target": "nsis",
      "icon": "src/renderer/assets/logo.ico",
      "artifactName": "VoxelXInstall.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "icon": "src/renderer/assets/logo.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "VoxelX",
      "deleteAppDataOnUninstall": false
    },
    "publish": {
      "provider": "github",
      "owner": "Zheni-Mai",
      "repo": "VoxelX"
    }
  }
}
